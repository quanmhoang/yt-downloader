{
  "name": "myria-services-nodes",
  "version": "0.1.0",
  "description": "Myriaverse Nodes microservice in NodeJS",
  "main": "server.js",
  "scripts": {
    "start": "npm run server",
    "server": "node build/src/server.js",
    "watch-node": "nodemon build/src/server.js",
    "build": "npm run gulp-tslint & npm run gulp-build",
    "gulp-build": "gulp build",
    "gulp-watch": "gulp watch",
    "gulp-tslint": "gulp tslint",
    "dev": "npm run gulp-build && npm run watch",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run gulp-watch\" \"npm run watch-node\"",
    "clean-build": "echo \"Cleaning build/** folder\" & gulp clean:build",
    "test": "npm run test:ci",
    "cucumber:single": "cucumber-js -f @cucumber/pretty-formatter -p default --name \"Give user their rewards\"",
    "test:single": "ENV=test PORT=8099 start-server-and-test dev http://0.0.0.0:8099 cucumber:single",
    "cucumber:default": "cucumber-js -f @cucumber/pretty-formatter -p default",
    "test:ci": "ENV=test PORT=8099 start-server-and-test dev http://0.0.0.0:8099 cucumber:default",
    "prepare": "husky install",
    "debug": "nodemon --exec node --inspect=0.0.0.0:9310 --require ts-node/register src/server.ts"
  },
  "author": "platfrom@myria.com",
  "license": "ISC",
  "dependencies": {
    "app-root-path": "^2.2.1",
    "aws-sdk": "^2.1086.0",
    "axios": "^0.26.0",
    "bignumber.js": "^9.1.1",
    "body-parser": "^1.19.0",
    "cli-table": "^0.3.1",
    "cookie-parser": "^1.4.6",
    "csv-parse": "^5.3.0",
    "ethers": "^5.7.0",
    "express": "^4.17.1",
    "express-list-endpoints": "^4.0.1",
    "ffmpeg-static": "^5.1.0",
    "jsonwebtoken": "^9.0.0",
    "myria-core-sdk": "^0.0.168-v5",
    "newrelic": "^8.13.2",
    "tyboost": "^2.0.0",
    "web3": "^1.3.0",
    "winston": "^3.2.1",
    "ytdl-core": "^4.11.4"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^8.7.0",
    "@cucumber/pretty-formatter": "^1.0.0",
    "@types/node": "^17.0.21",
    "assertthat": "^6.5.2",
    "concurrently": "^7.1.0",
    "cucumber-tsflow": "^4.0.0-preview.7",
    "del": "^4.1.1",
    "fast-csv": "^4.3.6",
    "gulp": "^4.0.2",
    "gulp-tslint": "^8.1.4",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^7.0.4",
    "nodemon": "^2.0.15",
    "start-server-and-test": "^1.14.0",
    "ts-node": "^10.9.1",
    "tslint": "^5.17.0",
    "typescript": "^3.5.1"
  },
  "pre-commit": [
    "gulp-tslint"
  ],
  "packageManager": "yarn@3.2.0"
}
